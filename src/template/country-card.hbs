<h1 class="card-text title">{{ name }}</h1>
<div class="card">
  <div class="card-body">
    <p class="card-text">Capital: {{ capital }}</p>
    <p class="card-text ">Population: {{ population }}</p>
    <p class="card-text">Region: {{ region }}</p>
    <ul class="languages-list">
      {{#each languages}}
      <li class="card-text">Languages: {{ this.name }}</li>
      {{/each}}
    </ul>
  </div>
  <div class="flag">
    <img src="{{flag}}" alt="{{name}}" width="400" />
  </div>
</div>